#################################################################
#    Rules
#################################################################

:ruolo rdf:type swrl:Variable .

:personaggio rdf:type swrl:Variable .

:premio rdf:type swrl:Variable .

:persona rdf:type swrl:Variable .

:serie rdf:type swrl:Variable .

tsao:s rdf:type swrl:Variable .

tsao:st rdf:type swrl:Variable .

tsao:e rdf:type swrl:Variable .

tsao:x rdf:type swrl:Variable .

tsao:anno rdf:type swrl:Variable .

[ swrla:isRuleEnabled "true"^^xsd:boolean ;
   rdfs:comment "Collega una SerieTV ad un premio vinto da un attore" ;
   rdfs:label "Serie ha vinto Premio" ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate :ruoloAttoriale ;
                           swrl:argument1 :ruolo ;
                           swrl:argument2 :personaggio
                         ] ;
               rdf:rest [ rdf:type swrl:AtomList ;
                          rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                      swrl:propertyPredicate :haRicevutoPremio ;
                                      swrl:argument1 :ruolo ;
                                      swrl:argument2 :premio
                                    ] ;
                          rdf:rest [ rdf:type swrl:AtomList ;
                                     rdf:first [ rdf:type swrl:ClassAtom ;
                                                 swrl:classPredicate :Persona ;
                                                 swrl:argument1 :persona
                                               ] ;
                                     rdf:rest [ rdf:type swrl:AtomList ;
                                                rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                            swrl:propertyPredicate :haRuolo ;
                                                            swrl:argument1 :persona ;
                                                            swrl:argument2 :ruolo
                                                          ] ;
                                                rdf:rest [ rdf:type swrl:AtomList ;
                                                           rdf:first [ rdf:type swrl:ClassAtom ;
                                                                       swrl:classPredicate :SerieTV ;
                                                                       swrl:argument1 :serie
                                                                     ] ;
                                                           rdf:rest [ rdf:type swrl:AtomList ;
                                                                      rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                                                  swrl:propertyPredicate :haPersonaggio ;
                                                                                  swrl:argument1 :serie ;
                                                                                  swrl:argument2 :personaggio
                                                                                ] ;
                                                                      rdf:rest rdf:nil
                                                                    ]
                                                         ]
                                              ]
                                   ]
                        ]
             ] ;
   swrl:head [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate :serieHaVintoPremio ;
                           swrl:argument1 :serie ;
                           swrl:argument2 :premio
                         ] ;
               rdf:rest rdf:nil
             ]
 ] .

[ swrla:isRuleEnabled "true"^^xsd:boolean ;
   rdfs:comment "Collega un episodio alla sua serie TV" ;
   rdfs:label "Serie ha episodio" ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:ClassAtom ;
                           swrl:classPredicate :SerieTV ;
                           swrl:argument1 tsao:s
                         ] ;
               rdf:rest [ rdf:type swrl:AtomList ;
                          rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                      swrl:propertyPredicate :haEpisodio ;
                                      swrl:argument1 tsao:st ;
                                      swrl:argument2 tsao:e
                                    ] ;
                          rdf:rest [ rdf:type swrl:AtomList ;
                                     rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                 swrl:propertyPredicate :haStagione ;
                                                 swrl:argument1 tsao:s ;
                                                 swrl:argument2 tsao:st
                                               ] ;
                                     rdf:rest rdf:nil
                                   ]
                        ]
             ] ;
   swrl:head [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate :eEpisodioDiSerie ;
                           swrl:argument1 tsao:e ;
                           swrl:argument2 tsao:s
                         ] ;
               rdf:rest rdf:nil
             ]
