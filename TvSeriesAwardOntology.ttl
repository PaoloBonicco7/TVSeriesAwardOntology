@prefix : <http://www.semanticweb.org/TvSeriesAwardOntology> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix wd: <http://www.wikidata.org/entity/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix tsao: <http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#> .
@prefix vann: <http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#vann:> .
@prefix swrla: <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix cpannotationschema: <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#> .
@base <http://www.semanticweb.org/TvSeriesAwardOntology> .

<http://www.semanticweb.org/TvSeriesAwardOntology> rdf:type owl:Ontology ;
                                                    owl:imports <http://www.ontologydesignpatterns.org/cp/owl/set.owl> ;
                                                    dc:creator "Paolo Bonicco" ;
                                                    dc:date "12-09-2024"@it ;
                                                    dc:description "Ontologia che modella le serie tv e i premi a loro associati, con le nomination e i premi che hanno vinto. Lo scopo dell'ontologia è raggruppare tutte le informazioni, i premi e le nomination delle principali serie tv degli ultimi anni."@it ;
                                                    dc:title "Tv Series Award Ontology" ;
                                                    :vann:preferredNamespacePrefix "tsao" ;
                                                    :vann:preferredNamespaceUri "http://www.semanticweb.org/TvSeriesAwardOntology" ;
                                                    rdfs:label "Tv Series Award Ontology"@it .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/TvSeriesAwardOntology#vann:preferredNamespacePrefix
:vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/TvSeriesAwardOntology#vann:preferredNamespaceUri
:vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/TvSeriesAwardOntology#candidaturaPer
:candidaturaPer rdf:type owl:ObjectProperty ;
                rdfs:domain :Ruolo .


###  http://www.semanticweb.org/TvSeriesAwardOntology#conRuolo
:conRuolo rdf:type owl:ObjectProperty ;
          owl:inverseOf :ruoloInSerie ;
          rdfs:domain :SerieTV ;
          rdfs:range :Ruolo ;
          rdfs:comment "Associa una serie TV ad un ruolo" ;
          rdfs:label "conRuolo" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#conferitoA
:conferitoA rdf:type owl:ObjectProperty ;
            owl:inverseOf :haRicevutoPremio ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :Premio ;
            rdfs:range :Ruolo ;
            rdfs:comment "Associa un Premio ad una Candidatura"@it ;
            rdfs:label "Conferito A"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#conferitoAPersona
:conferitoAPersona rdf:type owl:ObjectProperty ;
                   owl:inverseOf :haVintoPremio ;
                   rdfs:domain :Premio ;
                   rdfs:range :Persona ;
                   owl:propertyChainAxiom ( :conferitoA
                                            :ruoloAffidatoA
                                          ) ;
                   rdfs:comment "Associa un premio ad una persona che ricopre un ruolo, il quale ha ricevuto una candidatura associata a cui è stato conferito un premio"@it ;
                   rdfs:label "Conferito a Persona"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#conferitoNelloStessoEvento
:conferitoNelloStessoEvento rdf:type owl:ObjectProperty ,
                                     owl:SymmetricProperty ,
                                     owl:TransitiveProperty ;
                            rdfs:domain :Premio ;
                            rdfs:range :Premio ;
                            rdfs:comment "Mette in relazione due eventi che sono stati conferito nello stesso evento"@it ;
                            rdfs:label "Conferito Nello Stesso Evento"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#contiene
:contiene rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> ;
          owl:inverseOf :eContenutoDa ;
          rdfs:domain :Selezione ;
          rdfs:range :Ruolo ;
          rdfs:comment "Relazione tra una selezione per un premio e un ruolo"@it ;
          rdfs:label "Contiene"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#direttoDa
:direttoDa rdf:type owl:ObjectProperty ;
           owl:inverseOf :haDirettoEpisodio ;
           rdfs:domain :Episodio ;
           rdfs:range :RegistraShow ;
           rdfs:comment "Associa un Episodio ad un Regista che ha diretto l'episodio"@it ;
           rdfs:label "Diretto Da"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#eContenutoDa
:eContenutoDa rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> ;
              rdfs:domain :Ruolo ;
              rdfs:range :Selezione ;
              rdfs:comment "Mette in relazione un ruolo specifico e una selezione per un premio"@it ;
              rdfs:label "E' Contenuto Da"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#eEpisodioDiSerie
:eEpisodioDiSerie rdf:type owl:ObjectProperty ;
                  rdfs:domain :Episodio ;
                  rdfs:range :SerieTV ;
                  rdfs:comment "Associa un episodio alla sua serieTV"@it ;
                  rdfs:label "È episodio Serie"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#eStatoSceneggiatoDa
:eStatoSceneggiatoDa rdf:type owl:ObjectProperty ;
                     owl:inverseOf :haSceneggiatoEpisodio ;
                     rdfs:domain :Episodio ;
                     rdfs:range :SceneggiatoreShow ;
                     rdfs:comment "Mette in relazione un Episodio e il suo Sceneggiatore"@it ;
                     rdfs:label "E' Stato Sceneggiato Da"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#haCandidato
:haCandidato rdf:type owl:ObjectProperty ;
             rdfs:domain :Premio ;
             rdfs:range :Ruolo ;
             rdfs:comment "Associa un premio ad una candidatura" ;
             rdfs:label "haCandidato" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#haDirettoEpisodio
:haDirettoEpisodio rdf:type owl:ObjectProperty ;
                   rdfs:domain :RegistraShow ;
                   rdfs:range :Episodio ;
                   rdfs:comment "Identifica un Regista che ha diretto un Episodio"@it ;
                   rdfs:label "Ha Diretto Episodio"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#haEpisodio
:haEpisodio rdf:type owl:ObjectProperty ;
            rdfs:domain :Stagione ;
            rdfs:range :Episodio ;
            rdfs:comment "collega una stagione ai suoi episodi" ;
            rdfs:label "haEpisodio" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#haPersonaggio
:haPersonaggio rdf:type owl:ObjectProperty ;
               owl:inverseOf :personaggioDi ;
               rdfs:domain :SerieTV ;
               rdfs:range :Personaggio ;
               rdfs:comment "Lega una Serie TV and un personaggio"@it ;
               rdfs:label "Ha Personaggio"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#haProdotto
:haProdotto rdf:type owl:ObjectProperty ;
            rdfs:domain :ProduttoreShow ;
            rdfs:range :SerieTV ;
            owl:propertyChainAxiom ( :haRuolo
                                     :ruoloProduttore
                                   ) ;
            rdfs:comment "Associa un Produttore ad una Serie TV che ha prodotto"@it ;
            rdfs:label "haProdotto"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#haRicevutoPremio
:haRicevutoPremio rdf:type owl:ObjectProperty ;
                  rdfs:domain :Ruolo ;
                  rdfs:range :Premio ;
                  rdfs:comment "Associa una candidatura vincente ad un premio"@it ;
                  rdfs:label "Ha Ricevuto Premio"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#haRuolo
:haRuolo rdf:type owl:ObjectProperty ;
         owl:inverseOf :ruoloAffidatoA ;
         rdfs:domain :Persona ;
         rdfs:range :Ruolo ;
         rdfs:comment "Lega una persona al Ruolo che ricopre"@it ;
         rdfs:label "Ha Ruolo"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#haSceneggiatoEpisodio
:haSceneggiatoEpisodio rdf:type owl:ObjectProperty ;
                       rdfs:domain :SceneggiatoreShow ;
                       rdfs:range :Episodio ;
                       rdfs:comment "Identifica uno Sceneggiatore che ha collaborato alla sceneggiatura di un Episodio"@it ;
                       rdfs:label "Ha Sceneggiato Episodio"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#haSelezione
:haSelezione rdf:type owl:ObjectProperty ;
             owl:inverseOf :selezioneDi ;
             rdfs:domain :Premio ;
             rdfs:range :Selezione ;
             rdfs:comment "Lega un premio al proprio insieme di condidati unici, chiamata selezione per implicare l'assenza di duplicati all'interno dei candidati per un premio" ;
             rdfs:label "haSelezione" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#haSerieTV
:haSerieTV rdf:type owl:ObjectProperty ;
           rdfs:comment "collega una stagione as una serie TV" ;
           rdfs:label "haSerieTV" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#haStagione
:haStagione rdf:type owl:ObjectProperty ;
            rdfs:domain :SerieTV ;
            rdfs:range :Stagione ;
            rdfs:comment "indica che una serie TV ha una o più stagioni" ;
            rdfs:label "haStagione" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#haVintoPremio
:haVintoPremio rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <https://www.wikidata.org/wiki/Property:P166> ;
               rdfs:domain :Persona ;
               rdfs:range :Premio ;
               rdfs:comment "Associa una persona che ha vinto un premio al premio" ;
               rdfs:label "haVintoPremio" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#interpretatoDa
:interpretatoDa rdf:type owl:ObjectProperty ;
                owl:inverseOf :ruoloAttoriale ;
                rdfs:domain :Personaggio ;
                rdfs:range :Ruolo ;
                rdfs:comment "Associa un personaggio al suo ruolo"@it ;
                rdfs:label "Interpretato Da"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#nominationSerie
:nominationSerie rdf:type owl:ObjectProperty ;
                 rdfs:domain :SerieTV ;
                 rdfs:range :Premio ;
                 rdfs:comment "Collega una SerieTV ad un premio"@it ;
                 rdfs:label "Nomination per contenuto"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#personaggioDi
:personaggioDi rdf:type owl:ObjectProperty ;
               rdfs:domain :Personaggio ;
               rdfs:range :SerieTV ;
               rdfs:comment "Mette in relazione un Personaggio con la sua Serie TV"@it ;
               rdfs:label "Personaggio Di"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#premioConferito
:premioConferito rdf:type owl:ObjectProperty ;
                 rdfs:domain :CerimoniaPremiazione ;
                 rdfs:range :Premio ;
                 rdfs:comment "Associa un evento ad un premio assegnato durante una Cerimonia Di Premiazione"@it ;
                 rdfs:label "Premio Conferito"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#recitaIn
:recitaIn rdf:type owl:ObjectProperty ;
          rdfs:domain :Persona ;
          rdfs:range :SerieTV ;
          owl:propertyChainAxiom ( :haRuolo
                                   :ruoloAttoriale
                                   :personaggioDi
                                 ) ;
          rdfs:comment "Associa una persona che recita ad una SerieTV"@it ;
          rdfs:label "recitaIn"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#ruoloAffidatoA
:ruoloAffidatoA rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Ruolo ;
                rdfs:range :Persona ;
                rdfs:comment "Lega un Ruolo ad una Persona"@it ;
                rdfs:label "Ruolo Affidato A"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#ruoloAttore
:ruoloAttore rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :ruoloInSerie ;
             rdfs:domain :Attore ;
             rdfs:range :SerieTV ;
             owl:propertyChainAxiom ( :ruoloAttoriale
                                      :personaggioDi
                                    ) ;
             rdfs:comment "Lega un attore ad una Serie TV" ;
             rdfs:label "ruoloAttore" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#ruoloAttoriale
:ruoloAttoriale rdf:type owl:ObjectProperty ;
                rdfs:domain :Ruolo ;
                rdfs:range :Personaggio ;
                rdfs:comment "Mette in relazione un ruolo con il personaggio"@it ;
                rdfs:label "Ruolo Attoriale"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#ruoloInSerie
:ruoloInSerie rdf:type owl:ObjectProperty ;
              rdfs:domain :Ruolo ;
              rdfs:range :SerieTV .


###  http://www.semanticweb.org/TvSeriesAwardOntology#ruoloProduttore
:ruoloProduttore rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :ruoloInSerie ;
                 rdfs:domain :Produttore ;
                 rdfs:range :SerieTV ;
                 rdfs:comment "Lega un produttore ad una serie TV." ;
                 rdfs:label "ruoloProduttore" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#ruoloRegista
:ruoloRegista rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :ruoloInSerie ;
              rdfs:domain :Regista ;
              rdfs:range :SerieTV ;
              rdfs:comment "Lega un regista ad una serie TV." ;
              rdfs:seeAlso "ruoloRegista" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#ruoloSceneggiatore
:ruoloSceneggiatore rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :ruoloInSerie ;
                    rdfs:domain :Sceneggiatore ;
                    rdfs:range :SerieTV ;
                    rdfs:comment "Lega un sceneggaitore ad una serie TV." ;
                    rdfs:label "ruoloSceneggiatore" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#selezioneDi
:selezioneDi rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Selezione ;
             rdfs:range :Premio ;
             rdfs:comment "Lega una selezione, lista di candidati unici, ad un premio." ;
             rdfs:label "selezioneDi" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#serieHaEpisodio
:serieHaEpisodio rdf:type owl:ObjectProperty ;
                 rdfs:domain :SerieTV ;
                 rdfs:range :Episodio ;
                 rdfs:comment "Lega un Episodio ad una Serie TV"@it ;
                 rdfs:label "Serie Ha Episodio"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#serieHaVintoPremio
:serieHaVintoPremio rdf:type owl:ObjectProperty ;
                    rdfs:domain :SerieTV ;
                    rdfs:range :Premio ;
                    rdfs:comment "Collega un premio ad una serie TV"@it ;
                    rdfs:label "Serie Ha Vinto Premio"@it .


###  https://www.wikidata.org/wiki/Property:P166
<https://www.wikidata.org/wiki/Property:P166> rdf:type owl:ObjectProperty ;
                                              rdfs:domain :Persona ;
                                              rdfs:range :Premio ;
                                              rdfs:comment "premio assegnato a una persona o organizzazione"@it ;
                                              rdfs:isDefinedBy "https://www.wikidata.org/wiki/Property:P166" ;
                                              rdfs:label "premio ricevuto"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/TvSeriesAwardOntology#annoCerminoniaPremiazione
:annoCerminoniaPremiazione rdf:type owl:DatatypeProperty ,
                                    owl:FunctionalProperty ;
                           rdfs:domain :CerimoniaPremiazione ;
                           rdfs:range xsd:unsignedInt ;
                           rdfs:comment "Identifica l'anno in cui si è svolta una cerimonia di premiazione" ;
                           rdfs:label "annoCerimoniaPremiazione" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#annoInizioSerie
:annoInizioSerie rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :SerieTV ;
                 rdfs:range xsd:unsignedInt ;
                 rdfs:comment "L'anno in cui è andata in onda la prima stagione di una serie TV"@it ;
                 rdfs:label "Anno Inizio Serie"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#annoTermineSerie
:annoTermineSerie rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain :SerieTV ;
                  rdfs:range xsd:unsignedInt ;
                  rdfs:comment "Anno in cui è andata in onda l'ultima stagione di una Serie TV"@it ;
                  rdfs:label "Anno Termine Serie"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#categoria
:categoria rdf:type owl:DatatypeProperty ;
           owl:equivalentProperty <https://schema.org/category> ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :Premio ;
           rdfs:range xsd:string ;
           rdfs:comment "Usata per associare una categoria ai premi" ;
           rdfs:label "categoriaPremio" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#durataEpisodio
:durataEpisodio rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Episodio ;
                rdfs:range xsd:unsignedInt ;
                rdfs:comment "Indica la durata di un episodio espressa in minuti"@it ;
                rdfs:label "Durata Episodio"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#genere
:genere rdf:type owl:DatatypeProperty ;
        owl:equivalentProperty <https://schema.org/genre> ;
        rdf:type owl:FunctionalProperty ;
        rdfs:domain :SerieTV ;
        rdfs:range [ rdf:type rdfs:Datatype ;
                     owl:oneOf [ rdf:type rdf:List ;
                                 rdf:first "Commedia" ;
                                 rdf:rest [ rdf:type rdf:List ;
                                            rdf:first "Drammatica" ;
                                            rdf:rest [ rdf:type rdf:List ;
                                                       rdf:first "Miniserie" ;
                                                       rdf:rest rdf:nil
                                                     ]
                                          ]
                               ]
                   ] ;
        rdfs:comment "Identifica il genere di una serie TV" ;
        rdfs:label "genereSerie" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#nomeEpisodio
:nomeEpisodio rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Episodio ;
              rdfs:range xsd:string ;
              rdfs:comment "nome di un episodio di una serie TV" ;
              rdfs:label "nomeEpisodio" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#nomePersona
:nomePersona rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Persona ;
             rdfs:range xsd:string ;
             rdfs:comment "nome di una persona" ;
             rdfs:label "nomePersona" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#nomePersonaggio
:nomePersonaggio rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :Personaggio ;
                 rdfs:range xsd:string ;
                 rdfs:comment "nome di un personaggio di una serie TV" ;
                 rdfs:label "nomePersonaggio" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#nomeSerie
:nomeSerie rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :SerieTV ;
           rdfs:range xsd:string ;
           rdfs:comment "nome di una serie TV" ;
           rdfs:label "nomeSerie" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#numeroEpisodi
:numeroEpisodi rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :SerieTV ;
               rdfs:range xsd:int ;
               rdfs:comment "numero di episodi totali di una serie TV" ;
               rdfs:label "numeroEpisodi" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#numeroEpisodio
:numeroEpisodio rdf:type owl:DatatypeProperty ;
                rdfs:domain :Episodio ;
                rdfs:range xsd:int ;
                rdfs:comment "indica il numero sequenziale di un episodio all'interno di una stagione" ;
                rdfs:label "numeroEpisodio" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#numeroStagione
:numeroStagione rdf:type owl:DatatypeProperty ;
                rdfs:domain :Stagione ;
                rdfs:range xsd:int ;
                rdfs:comment "indica il numero sequenziale della stagione all'interno della serie" ;
                rdfs:label "numeroStagione" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#numeroStagioni
:numeroStagioni rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :SerieTV ;
                rdfs:range xsd:int ;
                rdfs:comment "rappresenta il numero di stagioni di una serie TV" ;
                rdfs:label "numeroStagioni" .


###  http://www.semanticweb.org/TvSeriesAwardOntology#sesso
:sesso rdf:type owl:DatatypeProperty ,
                owl:FunctionalProperty ;
       rdfs:domain :Persona ;
       rdfs:range [ rdf:type rdfs:Datatype ;
                    owl:oneOf [ rdf:type rdf:List ;
                                rdf:first "Femmina" ;
                                rdf:rest [ rdf:type rdf:List ;
                                           rdf:first "Maschio" ;
                                           rdf:rest [ rdf:type rdf:List ;
                                                      rdf:first "NonBinario" ;
                                                      rdf:rest rdf:nil
                                                    ]
                                         ]
                              ]
                  ] ;
       rdfs:comment "Proprietà che identifica il sesso di una persona"@it ;
       rdfs:label "sesso"@it .


###  http://www.semanticweb.org/TvSeriesAwardOntology#terminata
:terminata rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :SerieTV ;
           rdfs:range xsd:boolean ;
           rdfs:comment "Valore booleano per indicare se una Serie TV è ancora in corso o è teminata"@it ;
           rdfs:label "Terminata"@it .


###  https://schema.org/category
<https://schema.org/category> rdf:type owl:DatatypeProperty ,
                                       owl:FunctionalProperty ;
                              rdfs:comment "A category for the item. Greater signs or slashes can be used to informally indicate a category hierarchy." ;
                              rdfs:isDefinedBy "https://schema.org/category" ;
                              rdfs:label "category" .


###  https://schema.org/genre
<https://schema.org/genre> rdf:type owl:DatatypeProperty ,
                                    owl:FunctionalProperty ;
                           rdfs:comment "Genre of the creative work, broadcast channel or group."@en ;
