@prefix : <http://www.semanticweb.org/TvSeriesAwardOntology> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix wd: <http://www.wikidata.org/entity/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix tsao: <http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#> .
@prefix vann: <http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#vann:> .
@prefix swrla: <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix cpannotationschema: <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#> .
@base <http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology> .

<http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology> rdf:type owl:Ontology ;
                                                                                 owl:imports <http://www.ontologydesignpatterns.org/cp/owl/set.owl> ;
                                                                                 dc:creator "Paolo Bonicco" ;
                                                                                 dc:date "12-09-2024"@it ;
                                                                                 dc:description "Ontologia che modella le serie tv e i premi a loro associati, con le nomination e i premi che hanno vinto. Lo scopo dell'ontologia è raggruppare tutte le informazioni, i premi e le nomination delle principali serie tv degli ultimi anni."@it ;
                                                                                 dc:title "Tv Series Award Ontology" ;
                                                                                 vann:preferredNamespacePrefix "tsao" ;
                                                                                 vann:preferredNamespaceUri "http://www.semanticweb.org/TvSeriesAwardOntology" ;
                                                                                 rdfs:label "Tv Series Award Ontology"@it .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#vann:preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#vann:preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#candidaturaPer
tsao:candidaturaPer rdf:type owl:ObjectProperty ;
                    rdfs:domain tsao:Ruolo .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#conRuolo
tsao:conRuolo rdf:type owl:ObjectProperty ;
              owl:inverseOf tsao:ruoloInSerie ;
              rdfs:domain tsao:SerieTV ;
              rdfs:range tsao:Ruolo ;
              rdfs:comment "Associa una serie TV ad un ruolo" ;
              rdfs:label "conRuolo" .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#conferitoA
tsao:conferitoA rdf:type owl:ObjectProperty ;
                owl:inverseOf tsao:haRicevutoPremio ;
                rdf:type owl:FunctionalProperty ;
                rdfs:domain tsao:Premio ;
                rdfs:range tsao:Ruolo ;
                rdfs:comment "Associa un Premio ad una Candidatura"@it ;
                rdfs:label "Conferito A"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#conferitoAPersona
tsao:conferitoAPersona rdf:type owl:ObjectProperty ;
                       owl:inverseOf tsao:haVintoPremio ;
                       rdfs:domain tsao:Premio ;
                       rdfs:range tsao:Persona ;
                       owl:propertyChainAxiom ( tsao:conferitoA
                                                tsao:ruoloAffidatoA
                                              ) ;
                       rdfs:comment "Associa un premio ad una persona che ricopre un ruolo, il quale ha ricevuto una candidatura associata a cui è stato conferito un premio"@it ;
                       rdfs:label "Conferito a Persona"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#conferitoNelloStessoEvento
tsao:conferitoNelloStessoEvento rdf:type owl:ObjectProperty ,
                                         owl:SymmetricProperty ,
                                         owl:TransitiveProperty ;
                                rdfs:domain tsao:Premio ;
                                rdfs:range tsao:Premio ;
                                rdfs:comment "Mette in relazione due eventi che sono stati conferito nello stesso evento"@it ;
                                rdfs:label "Conferito Nello Stesso Evento"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#contiene
tsao:contiene rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> ;
              owl:inverseOf tsao:eContenutoDa ;
              rdfs:domain tsao:Selezione ;
              rdfs:range tsao:Ruolo ;
              rdfs:comment "Relazione tra una selezione per un premio e un ruolo"@it ;
              rdfs:label "Contiene"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#direttoDa
tsao:direttoDa rdf:type owl:ObjectProperty ;
               owl:inverseOf tsao:haDirettoEpisodio ;
               rdfs:domain tsao:Episodio ;
               rdfs:range tsao:RegistraShow ;
               rdfs:comment "Associa un Episodio ad un Regista che ha diretto l'episodio"@it ;
               rdfs:label "Diretto Da"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#eContenutoDa
tsao:eContenutoDa rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> ;
                  rdfs:domain tsao:Ruolo ;
                  rdfs:range tsao:Selezione ;
                  rdfs:comment "Mette in relazione un ruolo specifico e una selezione per un premio"@it ;
                  rdfs:label "E' Contenuto Da"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#eEpisodioDiSerie
tsao:eEpisodioDiSerie rdf:type owl:ObjectProperty ;
                      rdfs:domain tsao:Episodio ;
                      rdfs:range tsao:SerieTV ;
                      rdfs:comment "Associa un episodio alla sua serieTV"@it ;
                      rdfs:label "È episodio Serie"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#eStatoSceneggiatoDa
tsao:eStatoSceneggiatoDa rdf:type owl:ObjectProperty ;
                         owl:inverseOf tsao:haSceneggiatoEpisodio ;
                         rdfs:domain tsao:Episodio ;
                         rdfs:range tsao:SceneggiatoreShow ;
                         rdfs:comment "Mette in relazione un Episodio e il suo Sceneggiatore"@it ;
                         rdfs:label "E' Stato Sceneggiato Da"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#haCandidato
tsao:haCandidato rdf:type owl:ObjectProperty ;
                 rdfs:domain tsao:Premio ;
                 rdfs:range tsao:Ruolo ;
                 rdfs:comment "Associa un premio ad una candidatura" ;
                 rdfs:label "haCandidato" .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#haDirettoEpisodio
tsao:haDirettoEpisodio rdf:type owl:ObjectProperty ;
                       rdfs:domain tsao:RegistraShow ;
                       rdfs:range tsao:Episodio ;
                       rdfs:comment "Identifica un Regista che ha diretto un Episodio"@it ;
                       rdfs:label "Ha Diretto Episodio"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#haEpisodio
tsao:haEpisodio rdf:type owl:ObjectProperty ;
                rdfs:domain tsao:Stagione ;
                rdfs:range tsao:Episodio ;
                rdfs:comment "collega una stagione ai suoi episodi" ;
                rdfs:label "haEpisodio" .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#haPersonaggio
tsao:haPersonaggio rdf:type owl:ObjectProperty ;
                   owl:inverseOf tsao:personaggioDi ;
                   rdfs:domain tsao:SerieTV ;
                   rdfs:range tsao:Personaggio ;
                   rdfs:comment "Lega una Serie TV and un personaggio"@it ;
                   rdfs:label "Ha Personaggio"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#haProdotto
tsao:haProdotto rdf:type owl:ObjectProperty ;
                rdfs:domain tsao:ProduttoreShow ;
                rdfs:range tsao:SerieTV ;
                owl:propertyChainAxiom ( tsao:haRuolo
                                         tsao:ruoloProduttore
                                       ) ;
                rdfs:comment "Associa un Produttore ad una Serie TV che ha prodotto"@it ;
                rdfs:label "haProdotto"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#haRicevutoPremio
tsao:haRicevutoPremio rdf:type owl:ObjectProperty ;
                      rdfs:domain tsao:Ruolo ;
                      rdfs:range tsao:Premio ;
                      rdfs:comment "Associa una candidatura vincente ad un premio"@it ;
                      rdfs:label "Ha Ricevuto Premio"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#haRuolo
tsao:haRuolo rdf:type owl:ObjectProperty ;
             owl:inverseOf tsao:ruoloAffidatoA ;
             rdfs:domain tsao:Persona ;
             rdfs:range tsao:Ruolo ;
             rdfs:comment "Lega una persona al Ruolo che ricopre"@it ;
             rdfs:label "Ha Ruolo"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#haSceneggiatoEpisodio
tsao:haSceneggiatoEpisodio rdf:type owl:ObjectProperty ;
                           rdfs:domain tsao:SceneggiatoreShow ;
                           rdfs:range tsao:Episodio ;
                           rdfs:comment "Identifica uno Sceneggiatore che ha collaborato alla sceneggiatura di un Episodio"@it ;
                           rdfs:label "Ha Sceneggiato Episodio"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#haSelezione
tsao:haSelezione rdf:type owl:ObjectProperty ;
                 owl:inverseOf tsao:selezioneDi ;
                 rdfs:domain tsao:Premio ;
                 rdfs:range tsao:Selezione ;
                 rdfs:comment "Lega un premio al proprio insieme di condidati unici, chiamata selezione per implicare l'assenza di duplicati all'interno dei candidati per un premio" ;
                 rdfs:label "haSelezione" .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#haSerieTV
tsao:haSerieTV rdf:type owl:ObjectProperty ;
               rdfs:comment "collega una stagione as una serie TV" ;
               rdfs:label "haSerieTV" .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#haStagione
tsao:haStagione rdf:type owl:ObjectProperty ;
                rdfs:domain tsao:SerieTV ;
                rdfs:range tsao:Stagione ;
                rdfs:comment "indica che una serie TV ha una o più stagioni" ;
                rdfs:label "haStagione" .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#haVintoPremio
tsao:haVintoPremio rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf <https://www.wikidata.org/wiki/Property:P166> ;
                   rdfs:domain tsao:Persona ;
                   rdfs:range tsao:Premio ;
                   rdfs:comment "Associa una persona che ha vinto un premio al premio" ;
                   rdfs:label "haVintoPremio" .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#interpretatoDa
tsao:interpretatoDa rdf:type owl:ObjectProperty ;
                    owl:inverseOf tsao:ruoloAttoriale ;
                    rdfs:domain tsao:Personaggio ;
                    rdfs:range tsao:Ruolo ;
                    rdfs:comment "Associa un personaggio al suo ruolo"@it ;
                    rdfs:label "Interpretato Da"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#nominationSerie
tsao:nominationSerie rdf:type owl:ObjectProperty ;
                     rdfs:domain tsao:SerieTV ;
                     rdfs:range tsao:Premio ;
                     rdfs:comment "Collega una SerieTV ad un premio"@it ;
                     rdfs:label "Nomination per contenuto"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#personaggioDi
tsao:personaggioDi rdf:type owl:ObjectProperty ;
                   rdfs:domain tsao:Personaggio ;
                   rdfs:range tsao:SerieTV ;
                   rdfs:comment "Mette in relazione un Personaggio con la sua Serie TV"@it ;
                   rdfs:label "Personaggio Di"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#premioConferito
tsao:premioConferito rdf:type owl:ObjectProperty ;
                     rdfs:domain tsao:CerimoniaPremiazione ;
                     rdfs:range tsao:Premio ;
                     rdfs:comment "Associa un evento ad un premio assegnato durante una Cerimonia Di Premiazione"@it ;
                     rdfs:label "Premio Conferito"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#recitaIn
tsao:recitaIn rdf:type owl:ObjectProperty ;
              rdfs:domain tsao:Persona ;
              rdfs:range tsao:SerieTV ;
              owl:propertyChainAxiom ( tsao:haRuolo
                                       tsao:ruoloAttoriale
                                       tsao:personaggioDi
                                     ) ;
              rdfs:comment "Associa una persona che recita ad una SerieTV"@it ;
              rdfs:label "recitaIn"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#ruoloAffidatoA
tsao:ruoloAffidatoA rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain tsao:Ruolo ;
                    rdfs:range tsao:Persona ;
                    rdfs:comment "Lega un Ruolo ad una Persona"@it ;
                    rdfs:label "Ruolo Affidato A"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#ruoloAttore
tsao:ruoloAttore rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf tsao:ruoloInSerie ;
                 rdfs:domain tsao:Attore ;
                 rdfs:range tsao:SerieTV ;
                 owl:propertyChainAxiom ( tsao:ruoloAttoriale
                                          tsao:personaggioDi
                                        ) ;
                 rdfs:comment "Lega un attore ad una Serie TV" ;
                 rdfs:label "ruoloAttore" .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#ruoloAttoriale
tsao:ruoloAttoriale rdf:type owl:ObjectProperty ;
                    rdfs:domain tsao:Ruolo ;
                    rdfs:range tsao:Personaggio ;
                    rdfs:comment "Mette in relazione un ruolo con il personaggio"@it ;
                    rdfs:label "Ruolo Attoriale"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#ruoloInSerie
tsao:ruoloInSerie rdf:type owl:ObjectProperty ;
                  rdfs:domain tsao:Ruolo ;
                  rdfs:range tsao:SerieTV .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#ruoloProduttore
tsao:ruoloProduttore rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf tsao:ruoloInSerie ;
                     rdfs:domain tsao:Produttore ;
                     rdfs:range tsao:SerieTV ;
                     rdfs:comment "Lega un produttore ad una serie TV." ;
                     rdfs:label "ruoloProduttore" .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#ruoloRegista
tsao:ruoloRegista rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf tsao:ruoloInSerie ;
                  rdfs:domain tsao:Regista ;
                  rdfs:range tsao:SerieTV ;
                  rdfs:comment "Lega un regista ad una serie TV." ;
                  rdfs:seeAlso "ruoloRegista" .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#ruoloSceneggiatore
tsao:ruoloSceneggiatore rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf tsao:ruoloInSerie ;
                        rdfs:domain tsao:Sceneggiatore ;
                        rdfs:range tsao:SerieTV ;
                        rdfs:comment "Lega un sceneggaitore ad una serie TV." ;
                        rdfs:label "ruoloSceneggiatore" .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#selezioneDi
tsao:selezioneDi rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain tsao:Selezione ;
                 rdfs:range tsao:Premio ;
                 rdfs:comment "Lega una selezione, lista di candidati unici, ad un premio." ;
                 rdfs:label "selezioneDi" .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#serieHaEpisodio
tsao:serieHaEpisodio rdf:type owl:ObjectProperty ;
                     rdfs:domain tsao:SerieTV ;
                     rdfs:range tsao:Episodio ;
                     rdfs:comment "Lega un Episodio ad una Serie TV"@it ;
                     rdfs:label "Serie Ha Episodio"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#serieHaVintoPremio
tsao:serieHaVintoPremio rdf:type owl:ObjectProperty ;
                        rdfs:domain tsao:SerieTV ;
                        rdfs:range tsao:Premio ;
                        rdfs:comment "Collega un premio ad una serie TV"@it ;
                        rdfs:label "Serie Ha Vinto Premio"@it .


###  https://www.wikidata.org/wiki/Property:P166
<https://www.wikidata.org/wiki/Property:P166> rdf:type owl:ObjectProperty ;
                                              rdfs:domain tsao:Persona ;
                                              rdfs:range tsao:Premio ;
                                              rdfs:comment "premio assegnato a una persona o organizzazione"@it ;
                                              rdfs:isDefinedBy "https://www.wikidata.org/wiki/Property:P166" ;
                                              rdfs:label "premio ricevuto"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#annoCerminoniaPremiazione
tsao:annoCerminoniaPremiazione rdf:type owl:DatatypeProperty ,
                                        owl:FunctionalProperty ;
                               rdfs:domain tsao:CerimoniaPremiazione ;
                               rdfs:range xsd:unsignedInt ;
                               rdfs:comment "Identifica l'anno in cui si è svolta una cerimonia di premiazione" ;
                               rdfs:label "annoCerimoniaPremiazione" .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#annoInizioSerie
tsao:annoInizioSerie rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain tsao:SerieTV ;
                     rdfs:range xsd:unsignedInt ;
                     rdfs:comment "L'anno in cui è andata in onda la prima stagione di una serie TV"@it ;
                     rdfs:label "Anno Inizio Serie"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#annoTermineSerie
tsao:annoTermineSerie rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain tsao:SerieTV ;
                      rdfs:range xsd:unsignedInt ;
                      rdfs:comment "Anno in cui è andata in onda l'ultima stagione di una Serie TV"@it ;
                      rdfs:label "Anno Termine Serie"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#categoria
tsao:categoria rdf:type owl:DatatypeProperty ;
               owl:equivalentProperty <https://schema.org/category> ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain tsao:Premio ;
               rdfs:range xsd:string ;
               rdfs:comment "Usata per associare una categoria ai premi" ;
               rdfs:label "categoriaPremio" .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#durataEpisodio
tsao:durataEpisodio rdf:type owl:DatatypeProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain tsao:Episodio ;
                    rdfs:range xsd:unsignedInt ;
                    rdfs:comment "Indica la durata di un episodio espressa in minuti"@it ;
                    rdfs:label "Durata Episodio"@it .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#genere
tsao:genere rdf:type owl:DatatypeProperty ;
            owl:equivalentProperty <https://schema.org/genre> ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain tsao:SerieTV ;
            rdfs:range [ rdf:type rdfs:Datatype ;
                         owl:oneOf [ rdf:type rdf:List ;
                                     rdf:first "Commedia" ;
                                     rdf:rest [ rdf:type rdf:List ;
                                                rdf:first "Drammatica" ;
                                                rdf:rest [ rdf:type rdf:List ;
                                                           rdf:first "Miniserie" ;
                                                           rdf:rest rdf:nil
                                                         ]
                                              ]
                                   ]
                       ] ;
            rdfs:comment "Identifica il genere di una serie TV" ;
            rdfs:label "genereSerie" .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#nomeEpisodio
tsao:nomeEpisodio rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain tsao:Episodio ;
                  rdfs:range xsd:string ;
                  rdfs:comment "nome di un episodio di una serie TV" ;
                  rdfs:label "nomeEpisodio" .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#nomePersona
tsao:nomePersona rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain tsao:Persona ;
                 rdfs:range xsd:string ;
                 rdfs:comment "nome di una persona" ;
                 rdfs:label "nomePersona" .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#nomePersonaggio
tsao:nomePersonaggio rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain tsao:Personaggio ;
                     rdfs:range xsd:string ;
                     rdfs:comment "nome di un personaggio di una serie TV" ;
                     rdfs:label "nomePersonaggio" .


###  http://www.semanticweb.org/paolobonicco/ontologies/2023/TvSeriesAwardOntology#nomeSerie
tsao:nomeSerie rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain tsao:SerieTV ;
               rdfs:range xsd:string ;
               rdfs:comment "nome di una serie TV" ;
